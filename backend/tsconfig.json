{
	"compilerOptions": {
		"lib": ["es2023", "esnext.asynciterable"],
		"target": "es2022",
		"strict": true,
		"skipLibCheck": true,
		"module": "esnext",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"verbatimModuleSyntax": true,
		"jsx": "react-jsx",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
